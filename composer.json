{
    "name": "goksagun/scheduler-bundle",
    "description": "Scheduled task bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Burak Bolat",
            "email": "brkblt@gmail.com"
        }
    ],
    "keywords": [
        "scheduler task bundle"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php" : "^7.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "symfony/framework-bundle": "~2.7|~3.0",
        "symfony/console": "~2.7|~3.0",
        "symfony/yaml": "~2.7|~3.0",
        "mtdowling/cron-expression": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Goksagun\\SchedulerBundle\\": "",
            "exclude-from-classmap": [
                "/Tests/"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}