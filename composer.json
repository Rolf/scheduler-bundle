{
    "name": "goksagun/scheduler-bundle",
    "description": "Scheduled task bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Burak Bolat",
            "email": "brkblt@gmail.com"
        }
    ],
    "keywords": [
        "scheduler task bundle"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php" : "^7.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "symfony/framework-bundle": "~2.7|~3.0|~4.0",
        "symfony/console": "~2.7|~3.0|~4.0",
        "symfony/yaml": "~2.7|~3.0|~4.0",
        "symfony/process": "~2.7|~3.0|~4.0",
        "dragonmantank/cron-expression": "^2.0"
    },
    "autoload": {
        "psr-4": { "Goksagun\\SchedulerBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\Fixtures\\": "Tests/Fixtures/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "symfony": {
            "allow-contrib": "true"
        }
    },
    "minimum-stability": "dev"
}
